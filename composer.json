{
    "name": "sebastian/foal",
    "description": "Tool to find lines eliminated by OpCache's bytecode optimizer",
    "homepage": "https://github.com/sebastianbergmann/foal",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/sebastianbergmann/foal/issues"
    },
    "config": {
        "platform": {
            "php": "8.3.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "ext-zend-opcache": "*",
        "ext-vld": "*",
        "sebastian/cli-parser": "^3.0",
        "sebastian/diff": "^6.0",
        "sebastian/version": "^5.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "bin": [
        "foal"
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "0.3-dev"
        }
    }
}
